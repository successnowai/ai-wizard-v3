{
  "framework": "nextjs",
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "functions": {
    "app/api/wizard/step/*/route.ts": {
      "maxDuration": 60
    },
    "app/api/chat/*/route.ts": {
      "maxDuration": 30
    },
    "app/api/admin/*/route.ts": {
      "maxDuration": 45
    },
    "app/api/scraper/route.ts": {
      "maxDuration": 120
    }
  }
}
